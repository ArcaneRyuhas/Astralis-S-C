				<endpoint address="MessageService" binding="netTcpBinding" contract="DataAccessProject.Contracts.ILeaderboardManager"/>
				<host>
					<baseAddresses>
						<add baseAddress="http://192.168.100.38:8082" />
						<add baseAddress="net.tcp://192.168.100.38:8081" />
					</baseAddresses>
				</host>
			</service>
		</services>
	</system.serviceModel>
	<entityFramework>
